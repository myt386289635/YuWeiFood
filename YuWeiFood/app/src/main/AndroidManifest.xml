<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.dllo.yuweifood">




    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:largeHeap="true"
        android:hardwareAccelerated="false"
        android:allowBackup="true"
        android:icon="@mipmap/yuwei_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:configChanges="keyboardHidden|orientation|screenSize"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".mine.SignActivity"/>
        <activity android:name=".mine.collection.CollectionActivity"/>
        <activity android:name=".mine.collection.SystemActivity"/>
        <activity android:name=".mine.collection.NoticeActivity"/>


    </application>

</manifest>